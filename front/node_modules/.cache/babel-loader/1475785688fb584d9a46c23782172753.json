A,EAAA,EAAA;AACA,aAAA,SAAA,IAAA,EAAA;AACA,aAAA,cAAA;AAEA;AACA,KAjGA;AAkGA,IAAA,cAlGA,4BAkGA;AAAA;;AACA,UAAA,aAAA,GAAA,EAAA;AACA,UAAA,aAAA,GAAA,EAAA;AACA,UAAA,YAAA,GAAA,KAAA,YAAA;AACA,UAAA,YAAA,GAAA,KAAA,YAAA;AACA,UAAA,UAAA,GAAA,KAAA,UAAA,CAAA,IAAA,EAAA;AACA,UAAA,SAAA,GAAA,KAAA,SAAA;;AAEA,UAAA,KAAA,cAAA,EAAA;AACA,QAAA,aAAA,CAAA,IAAA,CAAA,WAAA;AACA;;AAEA,UAAA,KAAA,QAAA,EAAA;AACA,QAAA,aAAA,CAAA,IAAA,CAAA,MAAA,KAAA,QAAA;AACA;;AAEA,UAAA,KAAA,aAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,OAAA,GAAA,KAAA,aAAA,CAAA,KAAA,CAAA,OAAA,CAAA;AACA,QAAA,OAAA,GAAA,OAAA,CAAA,IAAA,EAAA;;AACA,YAAA,OAAA,IAAA,IAAA,EAAA;AACA,UAAA,aAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA;;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,KAAA,OAAA;AAEA,UAAA,OAAA,GAAA,KAAA,OAAA;;AAEA,UAAA,OAAA,IAAA,EAAA,EAAA;AACA,QAAA,OAAA,GAAA,CAAA,CAAA;AACA;;AACA,MAAA,KAAA,CACA,IADA,CACA,aADA,EACA;AACA,QAAA,aAAA,EAAA,aADA;AAEA,QAAA,aAAA,EAAA,aAFA;AAGA,QAAA,YAAA,EAAA,YAHA;AAIA,QAAA,YAAA,EAAA,YAJA;AAKA,QAAA,UAAA,EAAA,UALA;AAMA,QAAA,SAAA,EAAA,SANA;AAOA,QAAA,OAAA,EAAA;AAPA,OADA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA;AACA,OAbA,EAcA,KAdA,CAcA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAjBA;AAkBA;AAnJA,GA3BA;AAgLA,EAAA,QAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,aAAA,KAAA,kBAAA,GAAA,KAAA,OAAA,CAAA,MAAA;AACA,KAHA;AAKA,IAAA,SALA,uBAKA;AACA,aAAA,KAAA,gBAAA,IAAA,CAAA,IAAA,KAAA,OAAA,CAAA,MAAA,IAAA,CAAA;AACA;AAPA,GAhLA;AAyLA,EAAA,OAzLA,qBAyLA;AAAA;;AACA,SAAA,KAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,YAAA;AACA,MAAA,MAAA,CAAA,cAAA;AACA,KAFA;AAGA;AA7LA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"filter\">\n      <input\n        type=\"checkbox\"\n        v-model.lazy=\"filterEveryone\"\n        v-on:change=\"displayMessage\"\n      />\n      <label for=\"checkbox\"> @everyone </label>\n\n      <input type=\"checkbox\" v-model=\"filterMe\" v-on:change=\"displayMessage\" />\n      <label for=\"checkbox\"> @me </label>\n\n      <input\n        type=\"checkbox\"\n        v-model=\"filterMyPost\"\n        v-on:change=\"displayMessage\"\n      />\n      <label for=\"checkbox\"> My post </label>\n\n      <input\n        type=\"checkbox\"\n        v-model=\"filterFollow\"\n        v-on:change=\"displayMessage\"\n      />\n      <label for=\"checkbox\"> Followed </label>\n      <div></div>\n      <input\n        type=\"text\"\n        id=\"3\"\n        value=\"3\"\n        v-model=\"filterHashtag\"\n        placeholder=\"Search #\"\n        v-on:input=\"displayMessage\"\n      />\n\n      <input\n        type=\"text\"\n        id=\"3\"\n        value=\"3\"\n        v-model=\"userFilter\"\n        placeholder=\"Search User @\"\n        v-on:input=\"displayMessage\"\n      />\n    </div>\n\n    <div v-for=\"message in messages\" v-bind:key=\"message.id\">\n      <!-- PARTIE TWEET -->\n\n      <div v-if=\"message.retweet_user_id == null\">\n        <div class=\"Message\">\n          <div class=\"Message-information\">\n            <img\n              :src=\"'https://www.shareicon.net/data/128x128/2016/05/24/770117_people_512x512.png'\"\n              contain\n              height=\"20px\"\n              width=\"20px\"\n            />\n            <a>{{ message.username }}</a>\n            <div>\n              {{\n                new Date(message.message_date)\n                  .toLocaleString()\n                  .replace(/,/g, \" -\")\n              }}\n            </div>\n          </div>\n\n          <div class=\"Message-content\"></div>\n        </div>\n\n        <div></div>\n        <section></section>\n\n        <div>{{ message.message_content }}</div>\n\n        <button\n          v-if=\"message.username == username\"\n          @click=\"deleteMessage(message.message_id)\"\n        >\n          Delete\n        </button>\n\n        <button v-if=\"auth\" @click=\"likeMessage(message.message_id, user_id)\">\n          Like {{ message.liked }}\n        </button>\n        <button\n          v-if=\"auth\"\n          @click=\"retweetMessage(message.message_id, user_id)\"\n        >\n          Retweet\n        </button>\n        <button\n          v-if=\"auth && message.user_id != user_id\"\n          @click=\"followUser(message.user_id)\"\n        >\n          Follow {{ message.follow }}\n        </button>\n        <br />\n        <br />\n      </div>\n\n      <!-- PARTIE RETWEET -->\n\n      <div v-if=\"message.retweet_user_id != null\">\n        <div class=\"Message\">\n          <div class=\"Message-information\">\n            <!-- WHO RETWEETED ? -->\n            <div v-if=\"message.retweet_user_id != null\">\n              {{\n                message.retweet_username == username\n                  ? \"Vous\"\n                  : message.retweet_username\n              }}\n              a retweet√©\n            </div>\n            <!-- AVATAR -->\n\n            <img\n              :src=\"'https://www.shareicon.net/data/128x128/2016/05/24/770117_people_512x512.png'\"\n              contain\n              height=\"20px\"\n              width=\"20px\"\n            />\n            <!-- WHO WROTE THE MESSAGE -->\n            <a> @{{ message.username }} </a>\n\n            <!-- DATE -->\n            {{\n              new Date(message.message_date)\n                .toLocaleString()\n                .replace(/,/g, \" -\")\n            }}\n          </div>\n          <div class=\"Message-content\">{{ message.message_content }}</div>\n          <div class=\"Message-functions\">\n            <button\n              v-if=\"message.username == username\"\n              @click=\"deleteMessage(message.message_id)\"\n            >\n              Delete\n            </button>\n\n            <button\n              v-if=\"auth\"\n              @click=\"likeMessage(message.message_id, user_id)\"\n            >\n              Like {{ message.liked }}\n            </button>\n            <button\n              v-if=\"message.retweet_user_id == user_id\"\n              @click=\"delete_retweetMessage(message.message_id, user_id)\"\n            >\n              DeTweet\n            </button>\n            <button\n              v-if=\"auth && message.user_id != user_id\"\n              @click=\"followUser(message.user_id)\"\n            >\n              Follow {{ message.follow }}\n            </button>\n          </div>\n        </div>\n\n        <section></section>\n\n        <br />\n      </div>\n    </div>\n\n    <button @click=\"decrease_display_size()\">Show less</button>\n    <button @click=\"increase_display_size()\">Show more</button>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"DisplayMessage\",\n  data() {\n    return {\n      content: \"\",\n      message_max_length: 140,\n      messages: Array,\n      filterEveryone: false,\n      filterMe: false,\n      filterMyPost: false,\n      filterHashtag: \"\",\n      userFilter: \"\",\n      filterFollow: false,\n      limitPost: 10,\n    };\n  },\n  props: [\"user_id\", \"auth\", \"username\"],\n\n  created() {\n    var self = this;\n    function foo() {\n      self.displayMessage();\n      setTimeout(foo, 5000);\n      console.log(\"hey\");\n    }\n    foo();\n  },\n  methods: {\n    followUser(userid) {\n      axios\n        .post(\"/followUser\", {\n          follow: {\n            userid: this.user_id,\n            userToFollow_id: userid,\n          },\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    deleteMessage(messageID) {\n      axios\n        .post(\"/deleteMessage\", {\n          messageID,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    likeMessage(messageID, user_id) {\n      axios\n        .post(\"/likeMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    retweetMessage(messageID, user_id) {\n      axios\n        .post(\"/retweetMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    delete_retweetMessage(messageID, user_id) {\n      axios\n        .post(\"/delete_retweetMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    increase_display_size() {\n      this.limitPost += 10;\n      this.displayMessage();\n    },\n    decrease_display_size() {\n      if(i >10){\n        this.limitPost -= 10;\n        this.displayMessage();\n\n      }\n    },\n    displayMessage() {\n      var mentionFilter = [];\n      var hashtagFilter = [];\n      var followFilter = this.filterFollow;\n      var MyPostFilter = this.filterMyPost;\n      var userFilter = this.userFilter.trim();\n      var limitPost = this.limitPost;\n\n      if (this.filterEveryone) {\n        mentionFilter.push(\"@everyone\");\n      }\n\n      if (this.filterMe) {\n        mentionFilter.push(\"@\" + this.username);\n      }\n\n      if (this.filterHashtag.length > 0) {\n        var hashtag = this.filterHashtag.match(/#\\S+/g);\n        hashtag = hashtag.join();\n        if (hashtag != null) {\n          hashtagFilter.push(hashtag);\n        }\n      }\n\n      console.log(\"hola\" + this.user_id);\n\n      var user_id = this.user_id;\n\n      if (user_id == \"\") {\n        user_id = -1;\n      }\n      axios\n        .post(\"/getMessage\", {\n          mentionFilter,\n          hashtagFilter,\n          followFilter,\n          MyPostFilter,\n          userFilter,\n          limitPost,\n          user_id,\n        })\n        .then((res) => {\n          this.messages = res.data.array;\n          console.log(this.messages);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n  },\n  computed: {\n    getMessageLength() {\n      return this.message_max_length - this.content.length;\n    },\n\n    canSubmit() {\n      return this.getMessageLength >= 0 && this.content.length >= 0;\n    },\n  },\n  mounted() {\n    this.$root.$on(\"displayMessage\", () => {\n      this.displayMessage();\n    });\n  },\n};\n</script>\n"],"sourceRoot":"src/components/app"}]}