input\">\n          <textarea\n            id=\"inputMessage\"\n            placeholder=\"Quoi de neuf ?\"\n            v-model=\"content\"\n          ></textarea>\n        </div>\n\n        <div class=\"input-information\">\n          <div class=\"container1\">\n            <p id=\"charleft\">{{ getMessageLength }} caract√®res restants</p>\n          </div>\n          <div>\n            <br />\n            <br />\n          </div>\n\n          <div class=\"container2\">\n            <img v-if=\"canSubmit()\"\n              src=\"../../../src/assets/submit.png\"\n              contain\n              height=\"50px\"\n              width=\"50px\"\n              @click=\"submitMessage\"\n            />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport $ from \"jquery\";\nexport default {\n  name: \"InputMessage\",\n  data() {\n    return {\n      content: \"\",\n      message_max_length: 140,\n    };\n  },\n  props: [\"user_id\", \"auth\"],\n  methods: {\n    submitMessage() {\n      var currentDate = new Date().toLocaleString();\n\n      console.log(\"length : \" + this.content.length)\n      if(this.content.length >0){\n        axios.post(\"/postMessage\", {\n        message_content: this.content,\n        date: currentDate,\n        user_id: this.user_id,\n      });\n\n      this.$root.$emit(\"displayMessage\");\n      this.content = \"\";\n      }\n    },\n  },\n  computed: {\n    getMessageLength() {\n      return this.message_max_length - this.content.length;\n    },\n\n    canSubmit() {\n      return this.getMessageLength >= 0 && this.content.length >= 0;\n    },\n  },\n};\n</script>\n\n<style>\ntextarea {\n  width: 250px;\n  height: 85px;\n  resize: none;\n  outline: none;\n}\n\np {\n  margin: 0;\n  margin-left: 50;\n}\n\n.input-information {\n  display: flex;\n  flex-direction: row;\n}\n\n.container2 {\n  margin-left: 50px;\n}\n\n#inputMessage {\n  border: 2px solid #2c3e50;\n  