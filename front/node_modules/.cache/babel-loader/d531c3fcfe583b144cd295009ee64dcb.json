ACA,WAAA,SAAA,IAAA,EAAA;AACA,WAAA,cAAA;AACA,KA1FA;AA2FA,IAAA,qBA3FA,mCA2FA;AACA,UAAA,KAAA,SAAA,GAAA,EAAA,EAAA;AACA,aAAA,SAAA,IAAA,EAAA;AACA,aAAA,cAAA;AACA;AACA,KAhGA;AAiGA,IAAA,YAjGA,wBAiGA,IAjGA,EAiGA,CAAA,CAjGA;AAkGA,IAAA,cAlGA,4BAkGA;AAAA;;AACA,UAAA,aAAA,GAAA,EAAA;AACA,UAAA,aAAA,GAAA,EAAA;AACA,UAAA,YAAA,GAAA,KAAA,YAAA;AACA,UAAA,YAAA,GAAA,KAAA,YAAA;AACA,UAAA,UAAA,GAAA,KAAA,UAAA,CAAA,IAAA,EAAA;AACA,UAAA,SAAA,GAAA,KAAA,SAAA;;AAEA,UAAA,KAAA,cAAA,EAAA;AACA,QAAA,aAAA,CAAA,IAAA,CAAA,WAAA;AACA;;AAEA,UAAA,KAAA,QAAA,EAAA;AACA,QAAA,aAAA,CAAA,IAAA,CAAA,MAAA,KAAA,QAAA;AACA;;AAEA,UAAA,KAAA,aAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,OAAA,GAAA,KAAA,aAAA,CAAA,KAAA,CAAA,OAAA,CAAA;AACA,QAAA,OAAA,GAAA,OAAA,CAAA,IAAA,EAAA;;AACA,YAAA,OAAA,IAAA,IAAA,EAAA;AACA,UAAA,aAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA;;AAEA,UAAA,OAAA,GAAA,KAAA,OAAA;;AAEA,UAAA,OAAA,IAAA,EAAA,EAAA;AACA,QAAA,OAAA,GAAA,CAAA,CAAA;AACA;;AACA,MAAA,KAAA,CACA,IADA,CACA,aADA,EACA;AACA,QAAA,aAAA,EAAA,aADA;AAEA,QAAA,aAAA,EAAA,aAFA;AAGA,QAAA,YAAA,EAAA,YAHA;AAIA,QAAA,YAAA,EAAA,YAJA;AAKA,QAAA,UAAA,EAAA,UALA;AAMA,QAAA,SAAA,EAAA,SANA;AAOA,QAAA,OAAA,EAAA;AAPA,OADA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA;AACA,OAbA,EAcA,KAdA,CAcA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAjBA;AAkBA;AAjJA,GA7BA;AAgLA,EAAA,QAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,aAAA,KAAA,kBAAA,GAAA,KAAA,OAAA,CAAA,MAAA;AACA,KAHA;AAKA,IAAA,SALA,uBAKA;AACA,aAAA,KAAA,gBAAA,IAAA,CAAA,IAAA,KAAA,OAAA,CAAA,MAAA,IAAA,CAAA;AACA;AAPA,GAhLA;AAyLA,EAAA,OAzLA,qBAyLA;AAAA;;AACA,SAAA,KAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,YAAA;AACA,MAAA,MAAA,CAAA,cAAA;AACA,KAFA;AAGA;AA7LA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"filter\">\n      <div class=\"filter-container\">\n        <div class=\"test\">\n          <input\n            type=\"checkbox\"\n            v-model.lazy=\"filterEveryone\"\n            v-on:change=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n          <label for=\"checkbox\"> @EVERYONE </label>\n        </div>\n\n        <div class=\"test\">\n          <input\n            type=\"checkbox\"\n            v-model=\"filterMe\"\n            v-on:change=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n          <label for=\"checkbox\"> @ME </label>\n        </div>\n\n        <div class=\"test\">\n          <input\n            type=\"checkbox\"\n            v-model=\"filterMyPost\"\n            v-on:change=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n          <label for=\"checkbox\"> MY POSTS </label>\n        </div>\n        <div class=\"test\">\n          <input\n            type=\"checkbox\"\n            v-model=\"filterFollow\"\n            v-on:change=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n          <label for=\"checkbox\"> FOLLOWING </label>\n        </div>\n\n        <div class=\"test\">\n          <input\n            type=\"text\"\n            v-model=\"filterHashtag\"\n            placeholder=\"SEARCH HASHTAG\"\n            v-on:input=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n        </div>\n\n        <div class=\"test\">\n          <input\n            type=\"text\"\n            v-model=\"userFilter\"\n            placeholder=\"SEARCH USER\"\n            v-on:input=\"displayMessage\"\n            :disabled=\"!auth\"\n          />\n        </div>\n      </div>\n    </div>\n\n    <div class=\"body\">\n      <div v-if=\"auth\" class=\"header\">\n        <InputMessage\n          :user_id=\"user_id\"\n          :auth=\"auth\"\n          :username=\"username\"\n        ></InputMessage>\n      </div>\n\n      <div class=\"display-post\">\n        <div class=\"post\">\n          <div v-for=\"message in messages\" v-bind:key=\"message.id\" class=\"itsamessage\">\n            <!-- PARTIE TWEET -->\n\n            <div class=\"message-body\" v-if=\"message.retweet_user_id == null\">\n              <div class=\"Message\">\n                <div class=\"Message-information\">\n                  <img :src=\"require(`../../../src/assets/avatar/${message.username}.png`)\"\n                    contain\n                    height=\"35px\"\n                    width=\"35px\"\n                  />\n                  <a> @{{ message.username }}</a>\n                  <div class=\"date\">\n                    {{\n                      new Date(message.message_date)\n                        .toLocaleString()\n                        .replace(/,/g, \"  -\")\n                    }}\n                  </div>\n                \n                </div>\n\n                <div class=\"Message-content\"></div>\n              </div>\n\n              <div></div>\n              <section></section>\n\n              <div class=\"messagecontent\">{{ message.message_content }}</div>\n\n              <button\n                class=\"functions\"\n                v-if=\"message.username == username\"\n                @click=\"deleteMessage(message.message_id)\"\n              >\n                ‚ùå\n              </button>\n\n              <button\n                class=\"functions\"\n                v-if=\"auth\"\n                @click=\"likeMessage(message.message_id, user_id)\"\n              >\n                ‚ù§Ô∏è {{ message.liked }}\n              </button>\n              <button\n                class=\"functions\"\n                v-if=\"auth\"\n                @click=\"retweetMessage(message.message_id, user_id)\"\n              >\n                <img\n                  src=\"../../../src/assets/retweet.png\"\n                  contain\n                  height=\"15px\"\n                  width=\"15px\"\n                />\n              </button>\n              <button class=\"functions\"\n                v-if=\"auth && message.user_id != user_id\"\n                @click=\"followUser(message.user_id)\"\n              >\n                üë´ {{ message.follow }}\n              </button>\n              <br />\n              <br />\n            </div>\n\n            <!-- PARTIE RETWEET -->\n\n            <div class=\"message-body\" v-if=\"message.retweet_user_id != null\">\n              <div class=\"Message\">\n                <div class=\"Message-information\">\n                  <!-- WHO RETWEETED ? -->\n                  <div v-if=\"message.retweet_user_id != null\">\n                    {{\n                      message.retweet_username == username\n                        ? \"Vous\"\n                        : message.retweet_username\n                    }}\n                    a retweet√©\n                  </div>\n                  <!-- AVATAR -->\n                  <img :src=\"require(`../../../src/assets/avatar/${message.username}.png`)\"\n                    contain\n                    height=\"35px\"\n                    width=\"35px\"\n                  />\n                  <!-- WHO WROTE THE MESSAGE -->\n                  <a> @{{ message.username }} </a>\n\n                  <!-- DATE -->\n                  <div class=\"date\">{{\n                    new Date(message.message_date)\n                      .toLocaleString()\n                      .replace(/,/g, \" -\")\n                  }}</div>\n                  \n                </div>\n                <div class=\"Message-content\">{{ message.message_content }}</div>\n                <div class=\"Message-functions\">\n                  <button class=\"functions\"\n                    v-if=\"message.username == username\"\n                    @click=\"deleteMessage(message.message_id)\"\n                  >\n                    ‚ùå\n                  </button>\n\n                  <button class=\"functions\"\n                    v-if=\"auth\"\n                    @click=\"likeMessage(message.message_id, user_id)\"\n                  >\n                    ‚ù§Ô∏è {{ message.liked }}\n                  </button>\n                  <button \n                    class=\"functions\"\n                    v-if=\"message.retweet_user_id == user_id\"\n                    @click=\"delete_retweetMessage(message.message_id, user_id)\"\n                  >\n                    <img\n                      src=\"../../../src/assets/detweet.png\"\n                      contain\n                      height=\"16px\"\n                      width=\"16px\"\n                    />\n                  </button>\n                  <button class=\"functions\"\n                    v-if=\"auth && message.user_id != user_id\"\n                    @click=\"followUser(message.user_id)\"\n                  >\n                    üßë‚Äçü§ù‚Äçüßë {{ message.follow }}\n                  </button>\n                </div>\n              </div>\n\n              <br />\n\n              <div id=\"border-bottom\"></div>\n            </div>\n          </div>\n        </div>\n\n        <button v-if=\"messages.length > 10\" @click=\"decrease_display_size()\">\n          Show less\n        </button>\n        <button v-if=\"messages.length > 10\" @click=\"increase_display_size()\">Show more</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport InputMessage from \"./InputMessage.vue\";\n\nexport default {\n  name: \"DisplayMessage\",\n  components: {\n    InputMessage,\n  },\n  data() {\n    return {\n      content: \"\",\n      message_max_length: 140,\n      messages: Array,\n      filterEveryone: false,\n      filterMe: false,\n      filterMyPost: false,\n      filterHashtag: \"\",\n      userFilter: \"\",\n      filterFollow: false,\n      limitPost: 10,\n    };\n  },\n  props: [\"user_id\", \"auth\", \"username\"],\n\n  created() {\n    var self = this;\n    function loop_display() {\n      self.displayMessage();\n      setTimeout(loop_display, 5000);\n    }\n    loop_display();\n  },\n  methods: {\n    followUser(userid) {\n      axios\n        .post(\"/followUser\", {\n          follow: {\n            userid: this.user_id,\n            userToFollow_id: userid,\n          },\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    deleteMessage(messageID) {\n      axios\n        .post(\"/deleteMessage\", {\n          messageID,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    likeMessage(messageID, user_id) {\n      axios\n        .post(\"/likeMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    retweetMessage(messageID, user_id) {\n      axios\n        .post(\"/retweetMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    delete_retweetMessage(messageID, user_id) {\n      axios\n        .post(\"/delete_retweetMessage\", {\n          messageID,\n          user_id,\n        })\n        .then((res) => {\n          if (res.status == 200) {\n            this.displayMessage();\n          }\n          console.log(res.status);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n    increase_display_size() {\n      this.limitPost += 10;\n      this.displayMessage();\n    },\n    decrease_display_size() {\n      if (this.limitPost > 10) {\n        this.limitPost -= 10;\n        this.displayMessage();\n      }\n    },\n    set_everyone(data) {},\n    displayMessage() {\n      var mentionFilter = [];\n      var hashtagFilter = [];\n      var followFilter = this.filterFollow;\n      var MyPostFilter = this.filterMyPost;\n      var userFilter = this.userFilter.trim();\n      var limitPost = this.limitPost;\n\n      if (this.filterEveryone) {\n        mentionFilter.push(\"@everyone\");\n      }\n\n      if (this.filterMe) {\n        mentionFilter.push(\"@\" + this.username);\n      }\n\n      if (this.filterHashtag.length > 0) {\n        var hashtag = this.filterHashtag.match(/#\\S+/g);\n        hashtag = hashtag.join();\n        if (hashtag != null) {\n          hashtagFilter.push(hashtag);\n        }\n      }\n\n      var user_id = this.user_id;\n\n      if (user_id == \"\") {\n        user_id = -1;\n      }\n      axios\n        .post(\"/getMessage\", {\n          mentionFilter,\n          hashtagFilter,\n          followFilter,\n          MyPostFilter,\n          userFilter,\n          limitPost,\n          user_id,\n        })\n        .then((res) => {\n          this.messages = res.data.array;\n          console.log(this.messages);\n        })\n        .catch((err) => {\n          // what now?\n          console.log(err.response);\n        });\n    },\n  },\n  computed: {\n    getMessageLength() {\n      return this.message_max_length - this.content.length;\n    },\n\n    canSubmit() {\n      return this.getMessageLength >= 0 && this.content.length >= 0;\n    },\n  },\n  mounted() {\n    this.$root.$on(\"displayMessage\", () => {\n      this.displayMessage();\n    });\n  },\n};\n</script>\n\n<style>\n.container {\n  display: flex;\n  flex: 1;\n}\n\n.container .filter {\n  flex: 0 0 40%;\n  justify-content: flex-end;\n  border-right: 2px solid #2c3e50;\n  margin-right: 20px;\n\n  margin-left:auto;\n    margin-right:auto;\n}\n\n.container .filter .filter-container {\n  display: flex;\n  flex-direction: column;\n  float: right;\n  margin-right: 30px;\n}\n\n.container .filter .filter-container .test {\n  margin-bottom: 5px;\n}\n\n.container .body {\n  display: flex;\n  flex: 1;\n  flex-direction: column;\n  margin-left:30px;\n}\n\n\n\n.body .header {\n  height: 200px;\n}\n\n.body .display-post {\n  height: 100%;\n}\n\n.message-body {\n  width: 30%;\n  border-bottom: 2px solid #2c3e50;\n  margin-bottom: 20px;\n}\n\n.functions {\n  width: 50px;\n  height: 25px;\n}\n\n.date{\n  margin-bottom:10px;\n  font-size:14px;\n  font-style: italic;\n  margin-left:195px;\n}\n\n.functions{\n  margin-top:10px;\n}\n\n.itsamessage{\n  width:900px;\n\n}\n\n.messagecontent{\n   word-wrap: break-word;\n}\n</style>\n"],"sourceRoot":"src/components/app"}]}